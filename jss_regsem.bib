	Keywords = {Nonconvex penalty; Oblique structure; Rotation technique; Penalized likelihood factor analysis},
	Number = {0},
	Pages = {120--132},
	Title = {Estimation of an oblique structure via penalized likelihood factor analysis},
	Ty = {JOUR},
	Volume = {79},
